{
  "name": "donkey-card-game",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "16.14.1",
    "npm": "7.18.1"
  },
  "scripts": {
    "dev": "NODE_OPTIONS=--no-deprecation vite --host",
    "build": "NODE_OPTIONS=--no-deprecation vite build",
    "preview": "NODE_OPTIONS=--no-deprecation vite preview",
    "start": "npm run build && NODE_ENV=production go run ./cmd/server",
    "start:dev": "concurrently \"npm run dev\" \"go run ./cmd/server\"",
    "start:prod": "npm run build && NODE_ENV=production go run ./cmd/server",
    "server": "go run ./cmd/server",
    "server:prod": "NODE_ENV=production go run ./cmd/server",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "nvm": "nvm use"
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^4.5.1",
    "vitest": "^1.0.4"
  }
}
